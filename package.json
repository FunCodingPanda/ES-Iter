{
  "name": "es-iter",
  "version": "0.9.12",
  "description": "Itertools for JavaScript",
  "main": "build/Iter.js",
  "jsnext:main" : "src/Iter.js",
  "devDependencies": {
    "babel-cli" : "*",
    "babel-preset-es2015": "*",
    "babel-polyfill": "*",
    "babel-jest": "*",
    "jest-cli": "*"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "auto_mock_off.js"
    ],
    "testDirectoryName": "tests"
  },
  "scripts": {
    "test": "jest",
    "build": "babel --presets=es2015 src -d build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abozhilov/ES-Iter.git"
  },
  "keywords": [
    "itertools",
    "Iter",
    "iterator",
    "generator",
    "ES6"
  ],
  "author": "Asen Bozhilov <asen.bozhilov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abozhilov/ES-Iter/issues"
  },
  "homepage": "https://github.com/abozhilov/ES-Iter"
}
