'use strict';require('../auto_mock_off');require(
'babel/polyfill');var _srcItertools = require(
'../src/itertools');

describe('getIterator', function () {
    it('throws an error with non-iterable objects', function () {
        try {
            (0, _srcItertools.getIterator)(Object.create(null));} 
        catch (e) {
            expect(e instanceof TypeError);}});



    it('throws an error with no arguments', function () {
        try {
            (0, _srcItertools.getIterator)();} 
        catch (e) {
            expect(e instanceof TypeError);}});



    it('gets external iterator from iterable object', function () {
        var iter = (0, _srcItertools.getIterator)([1]);
        expect(iter.next().value).toBe(1);});


    it('gets external iterator from iterator', function () {
        var iter = (0, _srcItertools.getIterator)((0, _srcItertools.getIterator)([1, 2, 3]));
        expect(iter.next().value).toBe(1);});});